{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsamplefact"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"lsfailedstaging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsfailedstaging'"
		},
		"lssourceblb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lssourceblb'"
		},
		"lsstagecsvfile_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsstagecsvfile'"
		},
		"lsstagecsvfile_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureblbstrg.dfs.core.windows.net/"
		},
		"lsstaginputblb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "inputblbstorage"
		},
		"srckeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azure-keyvaultrscgrp.vault.azure.net/"
		},
		"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "usdssqlsrvr.database.windows.net"
		},
		"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "sauser"
		},
		"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"scheduletrigger_properties_adfactivities_parameters_testappendparameter": {
			"type": "string",
			"defaultValue": "est"
		},
		"default_properties_weburl_value": {
			"type": "string",
			"defaultValue": "https://www.dummy.restapiexample.com/"
		},
		"default_properties_stagingcorrectfile_value": {
			"type": "string",
			"defaultValue": "/rptsstageblob/correctsource"
		},
		"default_properties_stagingincorrectfile_value": {
			"type": "string",
			"defaultValue": "/rptsstageblob/wrongfiles"
		},
		"default_properties_sourcecontainername_value": {
			"type": "string",
			"defaultValue": "/rptsourcecontainer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfsamplefact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"lsfailedstaging_connectionString": {
						"value": "[parameters('lsfailedstaging_connectionString')]"
					},
					"lssourceblb_connectionString": {
						"value": "[parameters('lssourceblb_connectionString')]"
					},
					"lsstagecsvfile_accountKey": {
						"value": "[parameters('lsstagecsvfile_accountKey')]"
					},
					"default_properties_weburl_value": {
						"value": "[parameters('default_properties_weburl_value')]"
					},
					"default_properties_stagingcorrectfile_value": {
						"value": "[parameters('default_properties_stagingcorrectfile_value')]"
					},
					"default_properties_stagingincorrectfile_value": {
						"value": "[parameters('default_properties_stagingincorrectfile_value')]"
					},
					"default_properties_sourcecontainername_value": {
						"value": "[parameters('default_properties_sourcecontainername_value')]"
					},
					"lsstagecsvfile_properties_typeProperties_url": {
						"value": "[parameters('lsstagecsvfile_properties_typeProperties_url')]"
					},
					"srckeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('srckeyvault_properties_typeProperties_baseUrl')]"
					},
					"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('ssisironprm_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"lsstaginputblb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lsstaginputblb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfsamplefact_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"scheduletrigger_properties_adfactivities_parameters_testappendparameter": {
						"value": "[parameters('scheduletrigger_properties_adfactivities_parameters_testappendparameter')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfsamplefact_ArmTemplate_0')]"
			]
		}
	]
}