{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsamplefact"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"lsfailedstaging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsfailedstaging'"
		},
		"lssourceblb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lssourceblb'"
		},
		"lssqldblinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lssqldblinkedservice'"
